clc
clf
clear
num=3.17;
zeta = 0.59;
den=[1 2*zeta*sqrt(num) num];
m=50;
sys=tf(num,den)
omega=sqrt(den(3))
zeta=den(2)/(2*omega)
Ts=4/(zeta*omega);%3.8
Tp=pi/(omega*sqrt(1-zeta^2))
pos=exp(-zeta*pi/sqrt(1-zeta^2))*100 %10
t=0:0.1:m;
[yout,t]=step(sys,t);
max_y=max(yout)
yss=polyval(num,0)/polyval(den,0);
plot(t,yout);


